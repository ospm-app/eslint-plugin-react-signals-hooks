{
  "name": "@ospm/eslint-plugins",
  "version": "1.0.0",
  "type": "module",
  "description": "ESLint plugins",
  "scripts": {
    "tc": "tsc --project tsconfig.json --noEmit",
    "test": "npm run build && npm run test:all-rules",
    "lint": "npx eslint ./ --ext .ts",
    "lint:fix": "npx eslint ./ --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "bit:init": "bit init",
    "bit:install": "bit install",
    "bit:status": "bit status",
    "bit:compile": "bit compile",
    "bit:build": "bit build",
    "bit:test": "bit test",
    "bit:test:watch": "bit test --watch",
    "bit:lint": "bit lint",
    "bit:check-types": "bit check-types",
    "bit:create:node": "bit create node",
    "bit:create:react": "bit create react",
    "bit:tag": "bit tag --all",
    "bit:export": "bit export",
    "bit:import": "bit import",
    "bit:add": "bit add",
    "bit:install:pkg": "bit install",
    "bit:uninstall": "bit uninstall",
    "bit:update-deps": "bit install --check-files",
    "dev": "bit start",
    "dev:teambit": "bit start --teambit",
    "dev:ui": "bit start --ui",
    "dev:verbose": "bit start --verbose",
    "build": "bit build",
    "build:watch": "bit build --watch",
    "build:verbose": "bit build --verbose",
    "ci:install": "bit install --check-files",
    "ci:test": "bit test --watch=false",
    "ci:build": "bit build --no-cache",
    "ci:lint": "bit lint --fix",
    "ws:clean": "bit clean --hard && rm -rf node_modules && npm cache clean --force"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "best-practices"
  ],
  "author": {
    "name": "Alexey Lyakhov",
    "url": "https://github.com/JustFly1984",
    "email": "justfly1984@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ospm-app/eslint-plugin-react-signals-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/ospm-app/eslint-plugin-react-signals-hooks/issues"
  },
  "homepage": "https://github.com/ospm-app/eslint-plugin-react-signals-hooks#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-syntax-import-assertions": "^7.27.1",
    "@babel/preset-env": "7.28.0",
    "@biomejs/biome": "2.1.2",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.8",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@typescript-eslint/scope-manager": "^8.38.0",
    "@typescript-eslint/utils": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-es-x": "^9.0.0",
    "eslint-plugin-eslint-plugin": "^6.5.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-oxlint": "^1.8.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "oxlint": "1.8.0",
    "globals": "^16.3.0",
    "knip": "^5.62.0",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react-native": "^0.79.5"
  },
  "dependencies": {
    "@typescript-eslint/types": "^8.38.0"
  }
}
